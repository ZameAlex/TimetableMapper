@using TimeTableLibrary.FpmModels;
@model FpmUser
@{
	ViewData["Title"] = "Login";
}

@{
	Layout = "_LoginLayout.cshtml";
}

<body>
	<div class="col-lg-4"></div>
	<form asp-controller="Login" asp-action="Create" class="col-lg-4">
		<div class="form-group">
			<div>
				<label asp-for="Login"></label>
				<input type="text" asp-for="Login" class="form-control" />
			</div>
			<div>
				<label asp-for="Password"></label>
				<input type="password" asp-for="Password" class="form-control" />
			</div>
			<button type="submit" class="btn btn-default" asp-action="Create" >Submit</button>
		</div>
	</form>
	<br />
</body>
